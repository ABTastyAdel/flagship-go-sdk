#fs-sidemenu{position:fixed;z-index:2000;left:-420px;top:0;bottom:0;width:420px;overflow:visible;transition:1s ease left;padding:10px;background-color:#272727;color:#fff!important}#fs-sidemenu>.inner{overflow-y:auto;max-height:100%}#fs-sidemenu #fs-sidemenu-open{transform:rotate(90deg);width:80px;position:absolute;right:-60px;top:50%;margin-top:-40px;background-color:#272727;-webkit-appearance:none;border:none;color:#fff;padding:10px;cursor:pointer}#fs-sidemenu.opened{display:block;left:0;transition:1s ease left}#fs-sidemenu .fs-sidemenu-form label{color:#fff!important}#fs-sidemenu .fs-sidemenu-form .form-input{margin-bottom:5px}#fs-sidemenu .fs-sidemenu-form>button{position:absolute;bottom:10px;right:10px;background-color:#932154;background-image:linear-gradient(to right,#d01c6c,#ff6842);-webkit-appearance:none;border:none;color:#fff;padding:10px}#fs-sidemenu .fs-sidemenu-form label{min-width:120px;margin-right:10px}#fs-sidemenu .fs-sidemenu-form input{border:1px solid #fff;border-radius:4px;margin:0;color:#fff;display:inline-block;padding:6px 5px;min-width:0;background:0 0}#fs-sidemenu .fs-context-var{margin-top:10px}#fs-sidemenu .fs-context-var>div{margin-top:5px}#fs-sidemenu .fs-sidemenu-form button#fs-add-context{left:133px;right:auto;position:relative;margin-top:20px;background:0 0;border:1px solid #d01c6c;padding:7px}